{
  "name": "complexity-test",
	"displayName": "complexity-test",
	"description": "show complexity of editor content",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:complexity-test.documentReadingScore"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "complexity-test.documentReadingScore",
            "title": "Reading Score: Flesch-Kincaid Score for Document "
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.61.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "@vscode/test-electron": "^1.6.2"
    }

}
